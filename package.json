{
  "name": "chrome-xhr-request",
  "version": "0.1.2",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Element to send a request via Chrome extension as a proxy",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "chrome-xhr-request.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint chrome-xhr-request.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"chrome-xhr-request:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/chrome-xhr-request.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/chrome-xhr-request/issues",
    "email": "arc@mulesoft.com"
  }
}